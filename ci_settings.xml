<settings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://maven.apache.org/SETTINGS/1.1.0"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">
  <activeProfiles>
    <activeProfile>gitlab-maven</activeProfile>
  </activeProfiles>
  <profiles>
    <profile>
      <id>gitlab-maven</id>
      <pluginRepositories>
        <pluginRepository>
          <id>gitlab-maven</id>
          <releases>
            <checksumPolicy>warn</checksumPolicy>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <checksumPolicy>warn</checksumPolicy>
            <updatePolicy>never</updatePolicy>
          </snapshots>
          <url>${CI_API_V4_URL}/groups/6/-/packages/maven</url>
        </pluginRepository>
      </pluginRepositories>
      <repositories>
        <repository>
          <id>gitlab-maven</id>
          <releases>
            <checksumPolicy>warn</checksumPolicy>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <checksumPolicy>warn</checksumPolicy>
            <enabled>true</enabled>
            <updatePolicy>never</updatePolicy>
          </snapshots>
          <url>${CI_API_V4_URL}/groups/6/-/packages/maven</url>
        </repository>
      </repositories>
    </profile>
  </profiles>
  <servers>
    <server>
      <configuration>
        <httpHeaders>
          <property>
            <name>Job-Token</name>
            <value>${CI_JOB_TOKEN}</value>
          </property>
        </httpHeaders>
      </configuration>
      <id>gitlab-maven</id>
    </server>
  </servers>
</settings>
